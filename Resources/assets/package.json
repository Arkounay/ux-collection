{
    "name": "@arkounay/ux-collection",
    "description": "Collection for Symfony forms",
    "license": "MIT",
    "version": "2.2.1",
    "symfony": {
        "controllers": {
            "collection": {
                "main": "dist/controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "@arkounay/ux-collection/src/style.css": true,
                    "@arkounay/ux-collection/src/style-when-not-using-bootstrap-5.css": true
                }
            },
            "tabbed_collection": {
                "main": "dist/tabbed_controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true
            }
        }
    },
    "scripts": {
        "build": "babel src -d dist",
        "lint": "eslint src test"
    },
    "peerDependencies": {
        "sortablejs": "^1.14.0",
        "@hotwired/stimulus": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "sortablejs": "^1.14.0",
        "@hotwired/stimulus": "^3.0.0"
    }
}